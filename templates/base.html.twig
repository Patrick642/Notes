		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>
			{% block title %}
				Notes
			{% endblock %}
		</title>
		<link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico') }}">
